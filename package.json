{
  "name": "react-relay-router-koa",
  "version": "1.0.0",
  "description": "messing around",
  "main": "index.js",
  "babel": {
    "stage": 0,
    "sourceMaps": "inline",
    "plugins": [
      "babel-relay-plugin-loader"
    ]
  },
  "metadata": {
    "graphql": {
      "schema": "./graphql/schema/schema.json"
    }
  },
  "scripts": {
    "client:dev": "webpack-dev-server --hot --inline --progress --colors",
    "client:build": "webpack -p",
    "server:dev": "nodemon --exec babel-node -- server/index.js",
    "server:start": "babel-node server/index.js",
    "prod": "npm run client:build; npm run server:start",
    "dev": "webpack -w & npm run server:dev",
    "seed": "babel-node ./scripts/seed.js",
    "schema": "babel-node ./scripts/reloadSchema.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Alex Rattray <rattray.alex@gmail.com> (http://alexrattray.com/)",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-relay-plugin-loader": "^0.2.7",
    "babel-runtime": "^5.8.25",
    "graphql": "^0.4.7",
    "graphql-relay": "^0.3.3",
    "graphql-sequelize": "^0.11.4",
    "history": "^1.12.5",
    "koa": "^1.1.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^2.3.0",
    "koa-compress": "^1.0.8",
    "koa-generic-session": "^1.9.2",
    "koa-graphql": "^0.2.0",
    "koa-helmet": "^0.3.0",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-onerror": "^1.2.1",
    "koa-passport": "^1.2.0",
    "koa-router": "^5.2.3",
    "koa-static": "^1.5.1",
    "mysql": "^2.9.0",
    "passport-facebook": "^2.0.0",
    "passport-google-auth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-sequelize": "^0.6.0",
    "passport-twitter": "^1.0.3",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-helmet": "^1.1.5",
    "react-relay": "^0.4.0",
    "react-router": "^1.0.0-rc3",
    "react-router-relay": "^0.7.0",
    "sequelize": "^3.12.1",
    "sequelize-cli": "^2.0.0",
    "sqlite3": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-relay-plugin": "^0.3.0",
    "css-loader": "^0.19.0",
    "nodemon": "^1.7.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
